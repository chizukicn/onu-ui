{
  "name": "@onu-ui/monorepo",
  "version": "1.0.14-beta.2",
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nr volar:helper && nr stub",
    "docs": "npm -C docs run dev",
    "docs:build": "npm -C docs run build",
    "stub": "pnpm -r --parallel --filter=./packages/* run stub",
    "build": "rimraf packages/*/{dist,es,lib} && nr -r -F \"./packages/{preset,utils,components}\" build && nr volar:helper",
    "play": "run-s build play:run",
    "play:run": "run-p dev play:dev",
    "play:dev": "npm -C playground run dev",
    "play:build": "npm -C playground run build",
    "example": "npm -C examples/vite-onu-preset-only run dev",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "release": "bumpp package.json packages/**/package.json",
    "deploy": "nr build && nr play:build && nr docs:build",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:coverage": "vitest --coverage",
    "volar:helper": "esno scripts/volar-helper",
    "clean": "rimraf packages/*/{dist,es,lib}",
    "postinstall": "npx simple-git-hooks",
    "major": "taze minor -wIr"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.8",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/fluent-emoji": "^1.1.14",
    "@iconify-json/logos": "^1.1.35",
    "@onu-ui/components": "workspace:*",
    "@onu-ui/preset": "workspace:*",
    "@onu-ui/utils": "workspace:*",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.9",
    "@unocss/preset-mini": "^0.56.5",
    "@unocss/preset-uno": "^0.56.5",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.4.1",
    "bumpp": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "onu-ui": "workspace:*",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-inspect": "^0.7.38",
    "vitepress": "1.0.0-rc.10",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
